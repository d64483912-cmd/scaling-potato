{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/.next",
  "installCommand": "npm install --prefix . && npm install --prefix client",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/client/$1"
    }
  ]
}
